{
  "tasks": {
    "dev": "deno run --watch --unstable-kv --allow-net --allow-env --allow-read src/main.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "#shared/": "../shared/"
  },
  "deploy": {
    "project": "formicario",
    "exclude": [
      "**/node_modules"
    ],
    "include": [],
    "entrypoint": "src/main.ts"
  },
  "fmt": {
    "options": {
      "singleQuote": true
    }
  }
}
